    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "formidable": "^3.5.4",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.460.0",
    "next": "15.4.3",
    "postcss": "^8.5.6",
    "prisma": "^5.22.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.51.0",
